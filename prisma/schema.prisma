generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://main:12345678@aws-project-database.c25svdx5xwfo.us-east-1.rds.amazonaws.com:5432/projectdb"
}

model Alumno {
  id    Int     @id @default(autoincrement())
  nombres String  
  apellidos  String
  matricula String  @unique
  promedio Float
  fotoPerfilUrl String?
  password  String
}

model Profesor {
  id        Int     @id @default(autoincrement())
  numeroEmpleado Int  @unique
  nombres  String
  apellidos String
  horasClase Int
}
